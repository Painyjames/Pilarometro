{
  "webroot": "wwwroot",
  "version": "1.0.0-*",

  "dependencies": {
    "Microsoft.AspNet.Mvc": "6.0.0-beta8-*",
    "Microsoft.AspNet.Server.WebListener": "1.0.0-beta8-*",
    "Microsoft.AspNet.StaticFiles": "1.0.0-beta8-*",
    "Microsoft.Framework.Configuration": "1.0.0-beta8-*",
    "Microsoft.Framework.Logging": "1.0.0-beta8-*",
    "Microsoft.Framework.Logging.Console": "1.0.0-beta8-*",
    "Microsoft.AspNet.Mvc.WebApiCompatShim": "6.0.0-beta8-*",
    "Microsoft.AspNet.Hosting.Abstractions": "1.0.0-beta8-15618",
    "log4net": "1.2.11",
    "Autofac": "4.0.0-beta7-130",
    "Autofac.Framework.DependencyInjection": "4.0.0-beta7-130",
    "Hudl.Mjolnir": "2.2.0.0",
    "Newtonsoft.Json": "6.0.8.0",
    "Microsoft.Framework.OptionsModel": "1.0.0-beta8-*",
    "NEST": "1.7.1"
  },

  "commands": {
    "web": "Microsoft.AspNet.Hosting --ASPNET_ENV Dev --server Microsoft.AspNet.Server.WebListener --server.urls http://localhost:5000"
  },

  "frameworks": {
    "dnx451": {
    }
  },

  "publishExclude": [
    "node_modules",
    "bower_components",
    "**.xproj",
    "**.user",
    "**.vspscc"
  ],
  "exclude": [
    "wwwroot",
    "node_modules",
    "bower_components"
  ]
}
